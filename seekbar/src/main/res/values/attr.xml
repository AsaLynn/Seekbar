<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CursorSeekBar">
        <attr name="csbEnableLeftCursor" format="boolean" />
        <attr name="csbEnableRightCursor" format="boolean" />
        <attr name="csbSpaceBetween" format="dimension" />
        <attr name="csbAutoMoveDuration" format="integer" />
        <attr name="csbTextColor" format="color" />
        <attr name="csbTextColorSelected" format="color" />
        <attr name="csbBarColor" format="color" />
        <attr name="csbBarColorSelected" format="color" />
        <attr name="csbMarkTextArray" format="reference" />
        <attr name="csbLeftCursorDrawable" format="reference" />
        <attr name="csbRightCursorDrawable" format="reference" />
        <attr name="csbTextSize" format="dimension" />
        <attr name="csbMax" format="integer" />
        <attr name="csbUnit" format="string" localization="suggested" />
        <attr name="csbMarkTextColor" format="color" />
        <attr name="csbMarkTextSize" format="dimension" />
        <attr name="csbBarHeight" format="dimension" />
    </declare-styleable>

    <declare-styleable name="RangeSeekbar">
        <attr name="seekbarHeight" format="dimension" />
        <attr name="textSize" format="dimension" />
        <attr name="spaceBetween" format="dimension" />
        <attr name="leftCursorBackground" format="reference" />
        <attr name="rightCursorBackground" format="reference" />
        <attr name="markTextArray" format="reference" />
        <attr name="textColorNormal" format="color" />
        <attr name="textColorSelected" format="color" />
        <attr name="seekbarColorNormal" format="color" />
        <attr name="seekbarColorSelected" format="color" />
        <attr name="autoMoveDuration" format="integer" />
    </declare-styleable>

</resources>


